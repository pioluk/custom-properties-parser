{
  "name": "@pioluk/custom-properties-parser",
  "version": "0.1.0",
  "main": "./lib/index.js",
  "scripts": {
    "test": "tape test/*.js",
    "format": "prettier --write \"./{*.js,*.json,yml,yaml,{src,test}/**/*.{js,json,ts,tsx,css,less,scss,yml,yaml}}\""
  },
  "dependencies": {
    "postcss": "^7.0.17",
    "postcss-values-parser": "^3.0.5"
  },
  "devDependencies": {
    "husky": "^3.0.4",
    "lint-staged": "^9.2.3",
    "prettier": "^1.18.2",
    "tape": "^4.11.0"
  },
  "homepage": "https://github.com/pioluk/custom-properties-parser",
  "repository": {
    "type": "git",
    "url": "https://github.com/pioluk/custom-properties-parser"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,ts,tsx,css,less,scss,yml,yaml}": [
      "prettier --write",
      "git add"
    ]
  }
}
