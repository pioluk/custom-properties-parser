	"dependencies": {
		"postcss": "^8.0.9",
		"postcss-values-parser": "^5.0.0",
		"sass": "^1.45.0"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@total-typescript/ts-reset": "0.6.1",
		"@tsconfig/node18": "18.2.4",
		"@types/node": "22.9.0",
		"lefthook": "1.11.2",
		"pkgroll": "2.11.2",
		"rimraf": "6.0.1",
		"tsx": "4.19.3",
		"typescript": "5.8.2"
