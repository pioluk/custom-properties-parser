{
  "name": "@pioluk/custom-properties-parser",
  "version": "0.2.4",
  "main": "./lib/index.js",
  "typings": "./index.d.ts",
  "scripts": {
    "test": "tape test/*.js",
    "format": "prettier --write \"./{*.js,*.json,yml,yaml,{lib,src,test}/**/*.{js,json,ts,tsx,css,less,scss,yml,yaml}}\""
  },
  "dependencies": {
    "postcss": "^8.1.9",
    "postcss-values-parser": "^3.2.1",
    "sass": "^1.26.10"
  },
  "devDependencies": {
    "husky": "^4.2.5",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "tape": "^5.0.0"
  },
  "homepage": "https://github.com/pioluk/custom-properties-parser",
  "repository": {
    "type": "git",
    "url": "https://github.com/pioluk/custom-properties-parser"
  },
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,ts,tsx,css,less,scss,yml,yaml}": [
      "prettier --write"
    ]
  }
}
