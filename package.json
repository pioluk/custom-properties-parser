	"license": "MIT",
	"homepage": "https://github.com/pioluk/custom-properties-parser",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pioluk/custom-properties-parser.git"
	},
	"engines": {
		"node": ">= 18"
	},
	"exports": {
